<div class="container">
	<table class="table table-striped">
	    	<thead>
	    		<tr>
	    			<td>Nom:</td>
	    			<td>Descriptif:</td>
	    			<td>Lieu:</td>
	    			<td>Categorie:</td>
	    			<td>Montant:</td>
	    		</tr>
	    	</thead>
	    <% @projects.each do |project| %>
	    	<tbody>
	    		<tr>
	    			<td><%= project.name %></td>
	    			<td><%= project.description %></td>
	    			<td><%= project.place %></td>
	    			<td><%= project.category.name %></td>
	    			<td><%= project.amount %></td>
	    		</tr>
	    	</tbody>
	    <% end %>
	</table>
	
</div>